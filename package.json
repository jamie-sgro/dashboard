{
  "name": "dashboard",
  "version": "1.5.6",
  "private": true,
  "description": "This project involves building a one-page online dashboard for visualizing an SDG Cities Index that is both open-sourced and legally shareable.",
  "main": "index.js",
  "scripts": {
    "start": "git pull; npm install; npm run build; pm2 reload ./bin/www; pm2 monit",
    "start:dev": "tsc; node --require esm ./bin/www",
    "test": "tsc; mocha --require esm 'public/javascripts/tests/**/*'",
    "ci": "git pull; docker-compose up",
    "dev": "nodemon",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamie-sgro/dashboard.git"
  },
  "keywords": [
    "Dashboard",
    "Visualization",
    "SDGs",
    "Open Source"
  ],
  "author": "Jamie Sgro",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/Jamie.Sgro/dashboard/issues"
  },
  "homepage": "https://gitlab.com/Jamie.Sgro/dashboard#README",
  "dependencies": {
    "async": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "d3": "5.5.0",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "http": "0.0.0",
    "leaflet": "^1.5.1",
    "morgan": "^1.9.1",
    "npm": "^6.9.0",
    "request": "^2.88.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/d3": "5.5.0",
    "@types/jquery": "^3.5.5",
    "@types/leaflet": "^1.7.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "chai": "^4.3.4",
    "esm": "^3.2.25",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0"
  }
}
